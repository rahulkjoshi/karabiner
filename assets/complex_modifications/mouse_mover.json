{
  "title": "Use mouse side buttons to switch between Desktops",
  "rules": [
    {
      "description": "Maps button5 and button4 to ctrl+cmd+Left and ctrl+cmd+Right respectively",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "pointing_button": "button5"
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "left_control",
		"left_gui"
              ]
            }
          ]
        },
        {
          "type": "basic",
          "from": {
            "pointing_button": "button4"
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "left_control",
		"left_gui"
              ]
            }
          ]
        }
      ]
    }
  ]
}
